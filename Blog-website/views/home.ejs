  <%- include("partials/header"); -%>
    <h1> Home </h1>
    <p> <%= startingContent %> </p>

    <% posts.forEach(function(postObj) { %>

      <h1> <%= postObj.title %> </h1>
      <p>
        <%= postObj.content.substring(0, 100) + "..." %>
        <a href="/posts/<%=postObj.title%>">Read more</a>
      </p>

    <% }); %>


  <%- include("partials/footer"); -%>
